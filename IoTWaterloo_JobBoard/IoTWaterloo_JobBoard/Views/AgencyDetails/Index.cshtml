@model IEnumerable<IoTWaterloo_JobBoard.Models.AgencyDetails>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.AgencyName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AgencyWebsite)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AgencyPhone)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AgencyEmail)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.AgencyName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AgencyWebsite)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AgencyPhone)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AgencyEmail)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.AgencyId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.AgencyId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.AgencyId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
